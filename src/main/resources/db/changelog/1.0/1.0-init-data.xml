<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

    <changeSet id="1.0-init-data" author="Krzysztof Kwiatkowski">

        <loadData tableName="householdDTO" file="src/main/resources/src/main/resources/db/changelog/1.0/data/householdDTO.csv" separator=";" />
        <loadData tableName="userDTO" file="src/main/resources/db/changelog/1.0/data/userDTO.csv" separator=";">
            <column name="registration_date" header="created_date" type="timestamp" />
            <column name="last_failed_login" header="registration_date" type="timestamp" />
        </loadData>
        <loadData tableName="cashflowDTO" file="src/main/resources/db/changelog/1.0/data/cashflowDTO.csv" separator=";">
            <column name="start_date" header="created_date" type="timestamp" />
            <column name="end_date" header="registration_date" type="timestamp" />
        </loadData>

    </changeSet>

</databaseChangeLog>